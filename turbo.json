{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "build/**",
        "staticfiles/**"
      ]
    },
    "install": {
      "cache": false
    },
    "migrate": {
      "cache": false
    },
    "makemigrations": {
      "dependsOn": [
        "install"
      ],
      "cache": false
    },
    "dev": {
      "dependsOn": [
        "install"
      ],
      "cache": false,
      "persistent": true
    },
    "dev:network": {
      "dependsOn": [
        "install"
      ],
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "test": {},
    "start": {
      "dependsOn": [
        "install"
      ],
      "cache": false,
      "persistent": true
    },
    "@smarthome/backend#migrate": {
      "dependsOn": [
        "@smarthome/backend#install"
      ],
      "cache": false
    },
    "@smarthome/backend#dev": {
      "dependsOn": [
        "@smarthome/backend#migrate"
      ],
      "cache": false,
      "persistent": true
    }
  }
}